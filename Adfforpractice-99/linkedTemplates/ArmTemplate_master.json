{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Adfforpractice-99"
		},
		"AzureSynapseAnalyticsForAirBnB_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSynapseAnalyticsForAirBnB'"
		},
		"CosmosDbNoSql1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CosmosDbNoSql1'"
		},
		"ADLS_for_cust_data_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://airbnbdatacust.dfs.core.windows.net/"
		},
		"AzureSynapseAnalyticsForAirBnB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "synapsewsforpractice.sql.azuresynapse.net"
		},
		"AzureSynapseAnalyticsForAirBnB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "airBnBDWH"
		},
		"AzureSynapseAnalyticsForAirBnB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladminuser"
		},
		"SynapseLinkedNew_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "synapsewsforpractice.sql.azuresynapse.net"
		},
		"SynapseLinkedNew_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "airBnBDWH"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Adfforpractice-99_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSynapseAnalyticsForAirBnB_password": {
						"value": "[parameters('AzureSynapseAnalyticsForAirBnB_password')]"
					},
					"CosmosDbNoSql1_connectionString": {
						"value": "[parameters('CosmosDbNoSql1_connectionString')]"
					},
					"ADLS_for_cust_data_properties_typeProperties_url": {
						"value": "[parameters('ADLS_for_cust_data_properties_typeProperties_url')]"
					},
					"AzureSynapseAnalyticsForAirBnB_properties_typeProperties_server": {
						"value": "[parameters('AzureSynapseAnalyticsForAirBnB_properties_typeProperties_server')]"
					},
					"AzureSynapseAnalyticsForAirBnB_properties_typeProperties_database": {
						"value": "[parameters('AzureSynapseAnalyticsForAirBnB_properties_typeProperties_database')]"
					},
					"AzureSynapseAnalyticsForAirBnB_properties_typeProperties_userName": {
						"value": "[parameters('AzureSynapseAnalyticsForAirBnB_properties_typeProperties_userName')]"
					},
					"SynapseLinkedNew_properties_typeProperties_server": {
						"value": "[parameters('SynapseLinkedNew_properties_typeProperties_server')]"
					},
					"SynapseLinkedNew_properties_typeProperties_database": {
						"value": "[parameters('SynapseLinkedNew_properties_typeProperties_database')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}